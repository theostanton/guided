{
  "name": "guided",
  "private": true,
  "version": "0.0.1",
  "devDependencies": {
    "lerna": "^3.19.0"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "rm -rf node_modules && lerna clean -y",
    "deploy": "lerna run deploy && ./deploy.sh",
    "start:all": "lerna run start --parallel",
    "start:api:prod": "cd backend/api && yarn start:prod",
    "start:website": "lerna run start --parallel --ignore usersite",
    "start:usersite": "lerna run start --parallel --ignore website"
  }
}
