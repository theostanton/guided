{
  "name": "guided",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "develop": "STAGE=local yarn wsrun develop",
    "develop:frontend": "STAGE=local yarn workspace @guided/website run develop",
    "develop:backend": "STAGE=local yarn workspace @guided/backend run develop",
    "develop:graphql": "yarn workspace @guided/backend run develop",
    "build:types": "STAGE=local yarn workspace @guided/types run build",
    "deploy:frontend:staging": "yarn workspace @guided/website run deploy:staging",
    "deploy:backend:staging": "yarn workspace @guided/graphql run deploy:staging"
  },
  "dependencies": {
    "typescript": "next"
  },
  "devDependencies": {
    "wsrun": "^5.2.0"
  },
  "workspaces": {
    "packages": [
      "backend/*",
      "website"
    ]
  }
}
