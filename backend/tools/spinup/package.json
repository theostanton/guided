{
  "name": "@guided/spinup",
  "version": "0.0.0",
  "private": true,
  "main": "./srv/index.js",
  "types": "./srv/index.d.ts",
  "scripts": {
    "build": "yarn install && tsc",
    "watch": "tsc -w",
    "test": "jest",
    "action:drop": "ts-node src/invoke.ts drop",
    "action:truncate": "ts-node src/invoke.ts truncate",
    "action:1:roles": "ts-node src/invoke.ts roles",
    "action:2:create": "ts-node src/invoke.ts create",
    "action:3:privelages": "ts-node src/invoke.ts privelages",
    "start:local": "tsc && STAGE=local node srv/index.js",
    "start:production": "tsc && STAGE=production node srv/index.js",
    "start:staging": "STAGE=staging ts-node src/index.ts"
  },
  "devDependencies": {
    "@guided/database": "*",
    "slugify": "^1.3.6",
    "command-line-args": "^5.1.1",
    "csv-parse": "^4.8.5",
    "@types/command-line-args": "^5.0.0",
    "@types/listr": "^0.14.2",
    "listr": "^0.14.3",
    "ts-node": "^8.6.2"
  }
}